{
  "version": "8000",
  "inDir": "./dist",
  "outDir": "./dist-version",
  "config": "./ch2version.config.json",
  "outVersion": "./ch2version.version.json",
  "git": false,
  "language": "en",
  "clear": true,
  "rewrite": false,
  "prompt": false,
  "mode": "base",
  "ignore": [
    ".git/"
  ],
  "replaceDomain": {
    "readFileName": "dist",
    "baseURL": "/page",
    "output": [
      {
        "origin": "//app.example.com",
        "fileName": "index_app.html"
      },
      {
        "origin": "//wx.example.cn",
        "fileName": "index_wx.html"
      }
    ]
  }
}
