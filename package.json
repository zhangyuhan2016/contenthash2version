{
  "name": "contenthash2version",
  "version": "0.1.5",
  "description": "Convert the hash to version based on regular expressions",
  "main": "./bin/index.js",
  "bin": {
    "ch2version": "./bin/index.js"
  },
  "scripts": {
    "test": "node ./bin/index.js",
    "git-release": "standard-version",
    "git-log": "conventional-changelog ",
    "prepare": "husky install",
    "create-hook": "husky add .husky/pre-commit \"npm test\""
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "keywords": [
    "contenthash",
    "version"
  ],
  "author": "zhangyuhan2016",
  "license": "MIT",
  "devDependencies": {
    "commitizen": "^4.2.4",
    "commitlint": "^16.2.1",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^7.0.4",
    "standard-version": "^9.3.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zhangyuhan2016/contenthash2version.git"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.0.0",
    "i18n": "^0.14.2",
    "inquirer": "^8.2.1",
    "spinnies": "^0.5.1"
  }
}
